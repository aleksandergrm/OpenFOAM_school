FoamFile
{
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 45;

boundaryField
{
    inlet
	{
		type            freestream;
        freestreamValue $internalField;
	}
	
	outlet
	{
		type            freestream;
        freestreamValue $internalField;
	}

    wall       
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    hot_wall       
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    
    emptyFaces
    {
        type           empty;
    }
}


// ************************************************************************* //
